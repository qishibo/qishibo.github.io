<hr>
<!-- total pages -->
<small>total pages: {{paginator.total_pages}}<small> <br><br>

<!--previous page-->
{% if paginator.previous_page %}
    <a href="{{ paginator.previous_page_path | prepend: site.baseurl }}" class="btn btn-github"><span class="icon"></span>上一页</a>
{% endif %}

<!-- next page -->
{% if paginator.next_page_path %}
	<a href="{{ paginator.next_page_path | prepend: site.baseurl }}" class="btn btn-github"><span class="icon"></span>下一页</a>
{% endif %}
